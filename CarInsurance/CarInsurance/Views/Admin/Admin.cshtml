@using CarInsurance.ViewModels
@model List<InsureeVm>


<div class="row no-gutters m-3">
    <div class="card col-sm-12 col-md-10 col-lg-6 mx-auto shadow">
        <div class="card-header text-center">
            <h5 class="m-0">Quotes and Clients</h5>
        </div>
        <div class="card-body">
            @foreach (var client in Model)
            {
                
                    
                    <div class="row">
                        <div class="col">
                            @client.FirstName
                        </div>
                        <div class="col">
                            @client.LastName
                        </div>
                        <div class="col">
                            @client.EmailAddress
                        </div>
                        <div class="col">
                            @client.Quote
                        </div>
                    </div>
                

            }

        </div>
    </div>
</div>